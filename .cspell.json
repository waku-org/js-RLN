{
  "version": "0.1",
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
  "language": "en",
  "words": [
    "arrayify",
    "circom",
    "keypair",
    "merkle",
    "nwaku",
    "vkey",
    "Waku",
    "zerokit",
    "zkey"
  ],
  "flagWords": [],
  "ignorePaths": [
    "package.json",
    "package-lock.json",
    "yarn.lock",
    "tsconfig.json",
    "node_modules/**",
    "build",
    "gen",
    "proto",
    "*.spec.ts",
    "src/resources.ts"
  ],
  "patterns": [
    {
      "name": "import",
      "pattern": "/import .*/"
    }
  ],
  "ignoreRegExpList": ["import"]
}
